@model IEnumerable<WebApplication2.Models.Customer>

@{
    ViewBag.Title = "Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>
<p>
    @Html.ActionLink("New Customer", "New", "Customer", null, new { @class = "btn btn-primary" })
</p>
<table id="Customer_Id" class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>Customers</th>
        
    </tr>
    </thead>
        <tbody>
            @foreach(var customer in Model)
            {
                <tr>@Html.ActionLink(customer.Name,"Edit","Cutomers",new { id = customer.Customer_Id },null)</tr>
            }
        </tbody>
</table>
